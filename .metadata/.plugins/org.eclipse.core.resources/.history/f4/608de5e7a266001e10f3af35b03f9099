package ADO6PI;

import java.util.Scanner;

public class Ex1 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		
        System.out.println("informe um nmero");
		int voltas = ler.nextInt();

		while (voltas <= 5) {
			System.out.println(voltas);
			voltas++;
		}
		ler.close();

	}

}
